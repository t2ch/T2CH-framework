{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react-hooks/rules-of-hooks": 2,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "import/no-unresolved": 0
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "babel",
        "react-hooks",
        "prettier"
    ]
}