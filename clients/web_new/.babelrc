{
  "presets":
  [
    [
      "@babel/preset-env",
      {
        "targets":
        {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    [
      "babel-plugin-root-import", {
        "paths": [{
          "rootPathPrefix": "~",
          "rootPathSuffix": ""
        }, {
          "rootPathPrefix": "&",
          "rootPathSuffix": "src/"
        }]
      }
    ]
  ]
  
}